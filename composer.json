{
    "name": "isszz/think-third",
    "description": "Thinkphp third login",
    "keywords": [
        "think",
        "thinkphp",
        "third"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "isszz",
            "email": "isszz@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": ">=6.3",
        "topthink/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "isszz\\third\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-dev"
        },
        "think": {
            "config":{
                "third": "src/config/config.php"
            }
        }
    }
}